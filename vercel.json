{
  "version": 2,
  "builds": [
    {
      "src": "dist/apps/client/**",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "dist/apps/client"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/apps/client/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/apps/client/$1"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/dist/apps/client/index.html",
      "statusCode": 301
    }
  ]
}
