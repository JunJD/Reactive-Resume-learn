{
  "version": 2,
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "apps/server/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/apps/server/main.js"
    },
    {
      "src": "/artboard/(.*)",
      "dest": "dist/apps/artboard/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "dist/apps/client/index.html"
    }
  ]
}
